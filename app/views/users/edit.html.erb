<div class="view">
  <div class="form-elements">
    <%= form_for @user do |f| %>
      <p>
        <%= f.label :email %><br>
        <%= f.text_field :email %>
      </p>

      <p>
        <%= f.label :password %><br>
        <%= f.password_field :password %>
      </p>

      <p>
        <%= f.submit %>
      </p>
    <% end %>
  </div>
</div>
